{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "./src/env.d.ts",
    "./src/**/*",
    "./src/**/*.vue",
    "./*.ts",
    "../preload/*.d.ts",
    "./typed-router.d.ts",
    "../main/type.d.ts"
  ],
  "exclude": ["**/node_modules", "**/.*/"],
  "compilerOptions": {
    "composite": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*"
      ]
    }
  }
}
